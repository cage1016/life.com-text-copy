{
  "name": "life.com text copy",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "let me copy content",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.life.com.tw/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "http://www.life.com.tw/*"
  ]
}